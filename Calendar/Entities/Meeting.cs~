using System;
namespace Entities
{
	internal class Meeting
	{
        public Guid Id { get; set; }
        public string Title { get; set; }
        public string Details { get; set; }
        public DateTime StartTime { get; set; }
        public DateTime EndTime { get; set; }
        public List<string> Guests { get; set; }

        public Meeting()
        {
            Id = Guid.NewGuid();
        }
    }
}

