using System;
namespace Entities
{
    internal class Todo
    {
        public Guid Id { get; set; }
        public string Title { get; set; }
        public string Details { get; set; }
        public DateTime StartTime { get; set; }
        public DateTime EndTime { get; set; }
        public string Importance { get; set; }

        public Todo()
        {
        Id = Guid.NewGuid();
        }
    }
}

